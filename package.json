{
  "name": "felix-ui",
  "version": "0.2.5",
  "private": false,
  "description": "UI components for Vue.js based on Tailwind CSS ",
  "author": "Dhaval Budhelia <budhelia.dhaval@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "main": "./dist/felix-ui.umd.js",
  "module": "/dist/felix-ui.es.js",
  "files": [
    "dist/*",
    "src/*",
    "*.js"
  ],
  "exports": {
    ".": {
      "import": "./dist/felix-ui.es.js",
      "require": "./dist/felix-ui.umd.js"
    },
    "./dist/style.css": {
      "require": "./dist/style.css",
      "import": "./dist/style.css"
    }
  },
  "dependencies": {
    "dayjs": "^1.8.17",
    "vue": "^3.2.26"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@heroicons/vue": "^1.0.5",
    "@storybook/addon-actions": "^6.5.0-alpha.17",
    "@storybook/addon-essentials": "^6.5.0-alpha.17",
    "@storybook/addon-links": "^6.5.0-alpha.17",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3": "^6.5.0-alpha.17",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/component-compiler-utils": "^3.1.2",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^10.4.2",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.3",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.5",
    "storybook-builder-vite": "^0.1.13",
    "tailwindcss": "^3.0.13",
    "typescript": "^4.9.4",
    "vite": "^2.9.15",
    "vitest": "^0.29.8",
    "vue-loader": "^16.8.3"
  },
  "homepage": "https://github.com/dhavalbudhelia/felix-ui",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhavalbudhelia/felix-ui"
  }
}
